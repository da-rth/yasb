{
  "name": "yasb",
  "version": "0.1.0",
  "scripts": {
    "pretaurii": "npm run cargo:test && npm run lint",
    "ng": "ng",
    "start": "npm run tauri:serve",
    "web:serve": "ng serve --port 4200 --live-reload",
    "web:build": "ng build --base-href /",
    "web:dev": "npm run web:build",
    "web:prod": "npm run web:build -- -c production",
    "tauri": "tauri",
    "tauri:serve": "tauri dev",
    "tauri:bundle": "tauri build",
    "eslint": "eslint src/**/*.{js,ts,html} --max-warnings=0 --fix",
    "prettier": "npx prettier src/**/*.{js,ts,html,css,scss} --loglevel warn --end-of-line crlf --write",
    "rustfmt": "rustfmt ./src-tauri/src/main.rs",
    "format": "npm run eslint && npm run prettier && npm run rustfmt",
    "cargo:test": "cargo test --manifest-path ./src-tauri/Cargo.toml",
    "prepare": "husky install"
  },
  "dependencies": {
    "@angular/animations": "15.2.1",
    "@angular/cdk": "^15.2.1",
    "@angular/common": "15.2.1",
    "@angular/compiler": "15.2.1",
    "@angular/core": "15.2.1",
    "@angular/forms": "15.2.1",
    "@angular/language-service": "15.2.1",
    "@angular/material": "^15.2.1",
    "@angular/platform-browser": "15.2.1",
    "@angular/platform-browser-dynamic": "15.2.1",
    "@angular/router": "15.2.1",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@tauri-apps/api": "1.2.0",
    "humanize-duration": "^3.27.2",
    "material-icons": "^1.13.4",
    "ngx-json-viewer": "^3.2.1",
    "pretty-bytes": "^6.0.0",
    "rxjs": "7.8.0",
    "safe-eval": "^0.4.1",
    "safer-eval": "^1.3.6",
    "tslib": "^2.5.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.1",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "15.2.1",
    "@angular/compiler-cli": "15.2.1",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "@tauri-apps/cli": "1.2.3",
    "@types/humanize-duration": "^3.27.1",
    "@types/jasmine": "4.3.1",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "18.14.2",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "conventional-changelog-cli": "2.2.2",
    "eslint": "8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "39.9.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jasmine-core": "4.5.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "^2.8.4",
    "sass": "^1.54.4",
    "ts-node": "10.9.1",
    "typescript": "~4.9.5",
    "url": "^0.11.0",
    "wait-on": "7.0.1",
    "webdriver-manager": "12.1.8"
  }
}
